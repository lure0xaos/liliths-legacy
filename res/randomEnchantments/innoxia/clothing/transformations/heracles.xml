		<effect limit="2" mod1="TF_ASS" mod2="TF_MOD_SIZE_SECONDARY" potency="DRAIN" timer="0" type="CLOTHING"/>
	</itemEffects>
	
	<!-- 33% chance of rapid TF (hourly instead of daily) -->
	<itemEffects>
		<conditional><![CDATA[
			[#flags.getInt('rapidTF')<33]
		]]></conditional>
		<effect limit="0" mod1="TF_CORE" mod2="TF_MOD_FEMININITY" potency="MAJOR_DRAIN" timer="0" type="CLOTHING"/>
