<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<enchantment>
	<positiveEnchantment><![CDATA[false]]></positiveEnchantment>
	<applyDefaultSeal><![CDATA[true]]></applyDefaultSeal>
	
	<spawnWeighting><![CDATA[
		100
	]]></spawnWeighting>
	
	<namePrefix/>
	<namePostfix><![CDATA[of Heracles]]></namePostfix>
	<nameOverride/>
	
	<conditionalPreParsing><![CDATA[
		[#flags.setInt('rapidTF', RND.nextInt(100))]
	]]></conditionalPreParsing>
	
	<!-- 66% chance of normal TF speed (daily TFs) -->
	<itemEffects>
		<conditional><![CDATA[
			[#flags.getInt('rapidTF')>=33]
		]]></conditional>
		<effect limit="0" mod1="TF_CORE" mod2="TF_MOD_FEMININITY" potency="DRAIN" timer="0" type="CLOTHING"/>
		<effect limit="68" mod1="TF_CORE" mod2="TF_MOD_SIZE" potency="BOOST" timer="0" type="CLOTHING"/>
		<effect limit="100" mod1="TF_CORE" mod2="TF_MOD_SIZE_SECONDARY" potency="BOOST" timer="0" type="CLOTHING"/>
		<effect limit="100" mod1="TF_CORE" mod2="TF_MOD_SIZE_TERTIARY" potency="BOOST" timer="0" type="CLOTHING"/>
		
		<effect limit="0" mod1="TF_BREASTS" mod2="TF_MOD_SIZE" potency="DRAIN" timer="0" type="CLOTHING"/>
		<effect limit="0" mod1="TF_VAGINA" mod2="REMOVAL" potency="BOOST" timer="0" type="CLOTHING"/>
		
		<effect limit="0" mod1="TF_PENIS" mod2="TF_TYPE_1" potency="BOOST" timer="0" type="CLOTHING"/>
		<effect limit="26" mod1="TF_PENIS" mod2="TF_MOD_SIZE" potency="BOOST" timer="0" type="CLOTHING"/>
		<effect limit="4" mod1="TF_PENIS" mod2="TF_MOD_SIZE_SECONDARY" potency="BOOST" timer="0" type="CLOTHING"/>
