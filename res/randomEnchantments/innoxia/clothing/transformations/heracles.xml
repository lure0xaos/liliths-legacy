		<effect limit="0" mod1="TF_VAGINA" mod2="REMOVAL" potency="MAJOR_BOOST" timer="0" type="CLOTHING"/>
		
		<effect limit="0" mod1="TF_PENIS" mod2="TF_TYPE_1" potency="MAJOR_BOOST" timer="0" type="CLOTHING"/>
