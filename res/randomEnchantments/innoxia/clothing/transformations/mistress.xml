<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<enchantment>
	<positiveEnchantment><![CDATA[false]]></positiveEnchantment>
	<applyDefaultSeal><![CDATA[true]]></applyDefaultSeal>
	
	<spawnWeighting><![CDATA[
		100
	]]></spawnWeighting>
	
	<namePrefix><![CDATA[Mistress-making]]></namePrefix>
	<namePostfix/>
	<nameOverride/>
	
	<conditionalPreParsing><![CDATA[
		[#flags.setInt('rapidTF', RND.nextInt(100))]
	]]></conditionalPreParsing>
	
	<!-- 66% chance of normal TF speed (daily TFs) -->
	<itemEffects>
		<conditional><![CDATA[
			[#flags.getInt('rapidTF')>=33]
		]]></conditional>
		<effect limit="90" mod1="TF_CORE" mod2="TF_MOD_FEMININITY" potency="BOOST" timer="0" type="CLOTHING"/>
		<effect limit="9" mod1="TF_BREASTS" mod2="TF_MOD_SIZE" potency="BOOST" timer="0" type="CLOTHING"/>
		<effect limit="4" mod1="TF_ASS" mod2="TF_MOD_SIZE" potency="BOOST" timer="0" type="CLOTHING"/>
		<effect limit="4" mod1="TF_ASS" mod2="TF_MOD_SIZE_SECONDARY" potency="BOOST" timer="0" type="CLOTHING"/>
	</itemEffects>
	
	<!-- 33% chance of rapid TF (hourly instead of daily) -->
	<itemEffects>
		<conditional><![CDATA[
			[#flags.getInt('rapidTF')<33]
		]]></conditional>
		<effect limit="90" mod1="TF_CORE" mod2="TF_MOD_FEMININITY" potency="MAJOR_BOOST" timer="0" type="CLOTHING"/>
		<effect limit="9" mod1="TF_BREASTS" mod2="TF_MOD_SIZE" potency="MAJOR_BOOST" timer="0" type="CLOTHING"/>
		<effect limit="4" mod1="TF_ASS" mod2="TF_MOD_SIZE" potency="MAJOR_BOOST" timer="0" type="CLOTHING"/>
		<effect limit="4" mod1="TF_ASS" mod2="TF_MOD_SIZE_SECONDARY" potency="MAJOR_BOOST" timer="0" type="CLOTHING"/>
	</itemEffects>
	
	<itemEffects>
		<effect limit="0" mod1="TF_MOD_FETISH_BEHAVIOUR" mod2="TF_MOD_FETISH_DOMINANT" potency="MAJOR_BOOST" timer="0" type="CLOTHING"/>
		<effect limit="0" mod1="TF_MOD_FETISH_BEHAVIOUR" mod2="TF_MOD_FETISH_BONDAGE_APPLIER" potency="MAJOR_BOOST" timer="0" type="CLOTHING"/>
		<effect limit="0" mod1="TF_MOD_FETISH_BEHAVIOUR" mod2="TF_MOD_FETISH_SADIST" potency="BOOST" timer="0" type="CLOTHING"/>
		<effect limit="0" mod1="TF_MOD_FETISH_BEHAVIOUR" mod2="TF_MOD_FETISH_DENIAL" potency="BOOST" timer="0" type="CLOTHING"/>
	</itemEffects>
	
	<itemEffects>
		<conditional><![CDATA[
			[#game.isNonConEnabled()]
		]]></conditional>
		<effect limit="0" mod1="TF_MOD_FETISH_BEHAVIOUR" mod2="TF_MOD_FETISH_NON_CON_DOM" potency="BOOST" timer="0" type="CLOTHING"/>
	</itemEffects>
	
</enchantment>
